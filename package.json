{
  "name": "without-cdn",
  "version": "1.2.0",
  "description": "Web projects are deployed to a server, sometimes running on a LAN, and the CDN reference needs to be removed during deployment",
  "main": "lib/index.js",
  "scripts": {
    "uglify-js": "uglifyjs lib/index.js -o lib/index.js",
    "babel-js": "babel index.js -o lib/index.js",
    "build": "npm-run-all babel-js uglify-js"
  },
  "keywords": [
    "without-cdn",
    "download CDN file"
  ],
  "author": "cq0122",
  "homepage": "https://github.com/cq0122/without-cdn",
  "bugs": {
    "url": "https://github.com/cq0122/without-cdn/issues"
  },
  "license": "MIT",
  "bin": {
    "withoutcdn": "bin/index.js"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.24.1",
    "babel-cli": "^6.26.0"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.3",
    "commander": "^6.1.0",
    "fs-extra": "^9.0.1",
    "node-wget": "^0.4.3",
    "replace": "^1.2.0"
  }
}
